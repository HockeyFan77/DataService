{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-api",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/data-service-api"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-blazor",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/data-service-blazor/data-service-blazor.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}